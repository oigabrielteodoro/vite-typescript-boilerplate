{
  "name": "vite-typescript-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint --max-warnings=0",
    "lint:next": "next lint",
    "test": "NODE_ENV=test && jest -i --silent",
    "test:watch": "jest --maxWorkers=25% --watch",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prettier": "prettier --write '**/*.{ts,tsx,json,md}'",
    "prettier:check": "prettier -c '**/*.{ts,tsx,json,md}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint --fix",
      "yarn prettier --write",
      "yarn jest --findRelatedTests --bail"
    ],
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-private-methods": "7.16.11",
    "@babel/plugin-proposal-private-property-in-object": "7.16.7",
    "@babel/plugin-transform-react-jsx": "7.17.3",
    "@babel/preset-typescript": "7.16.7",
    "@storybook/addon-a11y": "6.4.22",
    "@storybook/addon-actions": "^6.4.0-alpha.34",
    "@storybook/addon-docs": "6.4.22",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-links": "^6.4.0-alpha.34",
    "@storybook/builder-vite": "0.1.33",
    "@storybook/react": "6.4.22",
    "@storybook/test-runner": "0.0.8",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/react-hooks": "8.0.0",
    "@testing-library/user-event": "14.1.1",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "@vitejs/plugin-react": "1.3.2",
    "babel": "6.23.0",
    "babel-loader": "8.2.5",
    "babel-plugin-styled-components": "2.0.7",
    "babel-plugin-transform-vite-meta-env": "1.0.3",
    "babel-preset-react-app": "10.0.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.2",
    "jest": "28.0.3",
    "jest-environment-jsdom": "28.0.2",
    "jest-styled-components": "7.0.8",
    "lint-staged": "11.1.2",
    "prettier": "2.4.0",
    "storybook": "6.3.8",
    "typescript": "4.6.4",
    "vite": "2.9.8",
    "vite-tsconfig-paths": "3.4.1"
  }
}
