{
  "name": "vite-typescript-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint --max-warnings=0",
    "lint:next": "next lint",
    "test": "NODE_ENV=test && jest -i --silent",
    "test:watch": "jest --maxWorkers=25% --watch",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "prettier": "prettier --write '**/*.{ts,tsx,json,md}'",
    "prettier:check": "prettier -c '**/*.{ts,tsx,json,md}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint --fix",
      "yarn prettier --write",
      "yarn jest --findRelatedTests --bail"
    ],
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-private-methods": "7.16.11",
    "@babel/plugin-proposal-private-property-in-object": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@storybook/addon-actions": "^6.4.0-alpha.34",
    "@storybook/addon-essentials": "^6.4.0-alpha.34",
    "@storybook/addon-links": "^6.4.0-alpha.34",
    "@storybook/react": "^6.4.0-alpha.34",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.4",
    "@testing-library/react-hooks": "7.0.2",
    "@types/react": "17.0.41",
    "@types/react-dom": "17.0.14",
    "@types/styled-components": "5.1.24",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "babel": "6.23.0",
    "babel-loader": "8.2.3",
    "babel-plugin-styled-components": "2.0.6",
    "babel-plugin-transform-vite-meta-env": "1.0.3",
    "babel-preset-react-app": "10.0.1",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.2",
    "jest": "27.5.1",
    "jest-styled-components": "7.0.8",
    "lint-staged": "11.1.2",
    "prettier": "2.4.0",
    "storybook": "6.3.8",
    "storybook-builder-vite": "^0.0.12",
    "typescript": "4.6.2",
    "vite": "2.8.6"
  }
}
